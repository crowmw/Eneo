@model IEnumerable<Eneo.Model.Models.ViewModels.User.UserAdminDisplay>

@{
    ViewBag.Title = "Moduł DMP";
}

<h2>Moduł DMP</h2>


<table class="table table-striped table-hover table-responsive">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            Lokalizacje
        </th>
        <th>
            Informacje o urządzeniu
        </th>
        <th>
            Informacje z facebooka
        </th>
    </tr>
    @foreach (var m in Model)
    {
        <tr>
            <td>
                @m.UserName
            </td>
            <td>
                <a href="@Url.Action("DMPPositions", new { userID = m.EneoUserID })">
                    <i class="icon-large icon-map-marker"></i>
                </a>
            </td>
            <td>
                <a href="@Url.Action("DMPDevice", new { userID = m.EneoUserID })">
                    <i class="icon-large  icon-iphone"></i>
                </a>
            </td>
            <td>
                <a href="@Url.Action("DMPFacebook", new { userID = m.EneoUserID })">
                    <i class="icon-large icon-facebook"></i>
                </a>
            </td>
        </tr>
    }

</table>



